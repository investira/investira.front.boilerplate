{
    "name": "investira.front.boilerplate",
    "version": "2.0.0",
    "author": "Investira",
    "description": "Boilerplate investira",
    "registry": true,
    "keywords": [
        "investira",
        "boilerplate"
    ],
    "private": true,
    "dependencies": {
        "@date-io/date-fns": "^2.14.0",
        "@date-io/moment": "1.3.13",
        "@emotion/react": "^11.7.1",
        "@emotion/styled": "^11.6.0",
        "@mui/lab": "^5.0.0-alpha.68",
        "@mui/material": "^5.4.1",
        "@mui/styles": "^5.4.1",
        "@mui/x-date-pickers": "^5.0.0-alpha.2",
        "autosuggest-highlight": "3.2.0",
        "classnames": "2.3.1",
        "d3-collection": "1.0.7",
        "d3-interpolate": "3.0.1",
        "d3-scale": "4.0.2",
        "d3-scale-chromatic": "3.0.0",
        "d3-selection": "3.0.0",
        "env-cmd": "10.1.0",
        "formik": "2.2.9",
        "idb": "6.1.5",
        "investira.react.charts": "file:../investira.react.charts",
        "investira.react.components": "file:../investira.react.components",
        "investira.react.lib": "file:../investira.react.lib",
        "investira.sdk": "2.3.5",
        "moment": "2.29.1",
        "node-sass": "4.14.1",
        "react": "17.0.2",
        "react-dom": "17.0.2",
        "react-input-mask": "2.0.4",
        "react-number-format": "4.5.2",
        "react-redux": "7.2.6",
        "react-router-dom": "5.2.0",
        "react-scripts": "4.0.3",
        "react-text-mask": "5.4.3",
        "react-transition-group": "4.3.0",
        "react-vis": "1.11.7",
        "redux": "4.1.2",
        "redux-pouchdb": "1.0.0-rc.3",
        "redux-thunk": "2.4.1",
        "scroll-behavior-polyfill": "2.0.13",
        "text-mask-addons": "3.8.0",
        "web-vitals": "2.1.4",
        "yup": "0.26.10"
    },
    "devDependencies": {
        "customize-cra": "1.0.0",
        "eslint-plugin-import-helpers": "^1.3.1",
        "eslint-plugin-prettier": "^4.0.0",
        "husky": "7.0.4",
        "lint-staged": "12.3.3",
        "prettier": "2.5.1",
        "prop-types": "15.7.1",
        "react-app-rewired": "2.2.1",
        "react-error-overlay": "6.0.9",
        "tape": "4.11.0"
    },
    "scripts": {
        "start": "PORT=3000 react-app-rewired start",
        "preload": "node ./scripts/preload.js",
        "meta": "node ./update_build.js",
        "stop": "react-app-rewired stop",
        "test": "react-app-rewired test --env=jsdom",
        "test:debug": "react-app-rewired --inspect-brk test --runInBand --no-cache",
        "eject": "react-app-rewired eject",
        "flow": "flow",
        "prepare": "husky install"
    },
    "browserslist": {
        "production": [
            "> 0.5%",
            "last 2 versions",
            "Firefox ESR",
            "not dead",
            "not IE 11",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "jshintConfig": {
        "curly": true,
        "eqeqeq": false,
        "latedef": false,
        "undef": false,
        "varstmt": true,
        "funcscope": true,
        "supernew": true,
        "boss": true,
        "eqnull": true,
        "strict": false,
        "scripturl": true,
        "maxdepth": 4,
        "maxparams": 5,
        "esversion": 9,
        "globals": {
            "esversion": 9,
            "location": true,
            "printStackTrace": false,
            "gLog": true,
            "gRC": true
        },
        "browser": true,
        "node": true
    },
    "resolutions": {
        "react-error-overlay": "6.0.9"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    }
}
